{
  "name": "internal-triggers",
  "version": "1.0.0",
  "description": "A Node.js server that hosts our internal triggers",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup",
    "dev": "tsx --require ./node_modules/dotenv/config src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@trigger.dev/integrations": "^0.1.13",
    "@trigger.dev/sdk": "^0.2.10",
    "pg": "^8.8.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@trigger.dev/tsconfig": "workspace:*",
    "@types/node": "16",
    "@types/pg": "^8.6.6",
    "dotenv": "^16.0.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}