{
  "name": "@trigger.dev/sdk",
  "version": "0.1.1",
  "description": "trigger.dev Node.JS SDK",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@trigger.dev/common-schemas": "workspace:*",
    "@trigger.dev/tsconfig": "workspace:*",
    "@types/debug": "^4.1.7",
    "@types/node": "16",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.3",
    "internal-bridge": "workspace:*",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "tsx": "^3.12.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:tsup",
    "build:tsup": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "evt": "^2.4.13",
    "ulid": "^2.3.0",
    "uuid": "^9.0.0",
    "ws": "^8.11.0",
    "zod": "^3.20.2"
  }
}
